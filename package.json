{
  "name": "desktop-version",
  "version": "1.0.0",
  "main": "main.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "discord-webhook-node": "^1.1.8",
    "discord.js": "^14.24.2",
    "node-fetch": "^3.3.2"
  },
  "build": {
    "appId": "Roblox Player",
    "productName": "Roblox Player",
    "icon": "./icon.ico",
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "mac": {
      "target": "dmg"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --watch * --exec electron .",
    "build": "electron-builder",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux"
  }
}
